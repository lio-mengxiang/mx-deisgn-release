{
  "name": "@mx-design/release",
  "version": "2.2.28",
  "description": "Lightweight release script for Front-end project",
  "keywords": [
    "mx-release",
    "mx release",
    "release script",
    "automated release",
    "js release script"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "license": "MIT",
  "author": "1334196450@qq.com",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "bin",
    "esm",
    "types"
  ],
  "scripts": {
    "build:types": "rimraf types && tsc --outDir types -d --emitDeclarationOnly",
    "build:es": "rimraf esm && mx buildLib --mode esm",
    "build:cjs": "rimraf lib && mx buildLib --mode cjs",
    "build": "yarn build:types && yarn build:cjs && yarn build:es",
    "clean": "rimraf esm lib types",
    "release": "ts-node ./scripts/release.ts"
  },
  "dependencies": {
    "@mx-design/cli": "1.0.13",
    "chalk": "^4.1.1",
    "conventional-changelog-cli": "^2.2.2",
    "inquirer": "8.2.0",
    "ora": "^5.1.0",
    "rimraf": "^3.0.2",
    "semver": "7.3.5",
    "typescript": "^4.5.4",
    "eslint": "^8.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react-hooks": "^4.0.8",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "ts-node": "10.7.0",
    "postcss-less": "4.0.1",
    "prettier": "^2.5.0",
    "pretty-quick": "^3.1.2"
  },
  "devDependencies": {
    "@types/node": "^15.3.0"
  },
  "browserslist": [
    "chrome 49",
    "Firefox 45",
    "safari 10"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  }
}